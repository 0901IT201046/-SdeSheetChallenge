Date of Submission:- 03/07/2022
Single Element in a Sorted Array*/


int uniqueElement(vector<int> arr, int n)
{
    int i,ans=0;
    for(i=0;i<n;)
    {
        if(arr[i]!=arr[i+1])
        {
            ans = arr[i];
            break;
        }
        else
        {
            i = i + 2;
        }
    }
    return ans;
}
